        <add assembly="DevExpress.PivotGrid.v15.2.Core, Version=7.2.3.0, Culture=neutral, PublicKeyToken=9B171C9FD64DA1D1" />
        <add assembly="DevExpress.XtraPivotGrid.v15.2, Version=7.2.3.0, Culture=neutral, PublicKeyToken=9B171C9FD64DA1D1" />
        <add assembly="DevExpress.Data.v15.2, Version=7.2.3.0, Culture=neutral, PublicKeyToken=9B171C9FD64DA1D1" />
